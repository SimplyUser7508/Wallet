{
    "manifest_version": 3,
    "name": "My Wallet Extension",
    "version": "1.0",
    "description": "A crypto wallet extension for Ethereum",
    "background": {
      "service_worker": "background.js"
    },
    "permissions": [
      "storage",
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "128": "icons/icon-192.png"
      }
    }
  }
  